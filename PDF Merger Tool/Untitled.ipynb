    "import PyPDF2\n",
    "import os\n",
    "\n",
    "List of PDFs to merge\n",
    "pdfs = ['file1.pdf', 'file2.pdf', 'file3.pdf']\n",
    "\n",
    "merger = PyPDF2.PdfMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(pdf)\n",
    "\n",
    "merger.write(\"merged_output.pdf\")\n",
    "merger.close()"
   ]
  }
